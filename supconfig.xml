<?xml version="1.0" encoding="UTF-8"?>
<supWSD xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="supconfig.xsd">
	<working_directory>semeval13</working_directory>
	<parser mns="">semeval13</parser>
	<preprocessing>
		<splitter model="">open_nlp</splitter>
		<tokenizer model="">open_nlp</tokenizer>
		<tagger model="">open_nlp</tagger>
		<lemmatizer model="">open_nlp</lemmatizer>
		<dependency_parser model="">none</dependency_parser>
	</preprocessing>
	<extraction>
		<features>
			<pos_tags cutoff="0">true</pos_tags>
			<local_collocations cutoff="0">true</local_collocations>
			<surrounding_words cutoff="0" window="1">true</surrounding_words>
			<word_embeddings strategy="FRA" window="4"
				vectors="vectorsCBOWEng.txt"
				vocab="vocabCBOWEng.txt" cache="1">false</word_embeddings>
			<syntactic_relations>false</syntactic_relations>
		</features>
	</extraction>
	<classifier>liblinear</classifier>
	<writer>all</writer>
	<sense_inventory
		dict="">wordnet</sense_inventory>
</supWSD>
